<div class="wrapper">
  <h1>{{name}}</h1>
  <p>{{product_data.model}}</p>
  <p>{{product_data.brand}}</p>
  <h2>Categories</h2>
  <ul>
  {{#each categories}}
    <li>{{id}}-{{name}}</li>
  {{/each}}
  </ul>
  <h2>Reviews</h2>
  <ul>
  {{#each reviews}}
    <li>{{body}}</li>
  {{/each}}
  </ul>
</div>
